@{
    Layout = "_SimpleLayout";
    ViewData["Title"] = "Students";

    ViewData["Students"] = new List<string>()
    {
        "Bob","Sue","Dave","Rob","Eric","Mike"
    };
}
<h1>@ViewData["Title"]</h1>

<p>Here are all the students</p>
<ul>
@foreach(var s in ViewBag.Students)
{
    <li>@s</li>
}
</ul>